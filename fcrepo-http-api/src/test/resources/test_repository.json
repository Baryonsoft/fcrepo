{
  "name" : "repo",
  "jndiName" : "",
  "workspaces" : {
    "predefined" : [],
    "default" : "default",
    "allowCreation" : true
  },
  "storage" : {
    "persistence" : {
      "type" : "file",
      "path" : "target/fedora_repository/store"
    }
  },
  "externalSources" : {
    "fileSystem" : {
      "classname" : "org.fcrepo.connector.file.FedoraFileSystemConnector",
      "directoryPath" : "target/test-classes/test-objects",
      "readonly" : false,
      "extraPropertiesStorage": "json",
      "cacheable" : false,
      "projections" : [ "default:/files => /" ]
    },
    "readonly" : {
      "classname" : "org.fcrepo.connector.file.FedoraFileSystemConnector",
      "directoryPath" : "target/test-classes/test-objects",
      "propertiesDirectoryPath" : "target/test-classes/test-meta",
      "readonly" : true,
      "extraPropertiesStorage": "none",
      "cacheable" : false,
      "projections" : [ "default:/readonlyfiles => /" ]
    }
  },
  "security" : {
    "anonymous" : {
      "roles" : ["readonly","readwrite","admin"],
      "useOnFailedLogin" : false
    },
    "providers" : [
      { "classname" : "org.fcrepo.auth.common.BypassSecurityServletAuthenticationProvider" }
    ]
  },
  "indexProviders" : {
    "lucene" : {
      "classname" : "lucene",
      "directory" : "target/indexes/lucene"
    }
  },
  "indexes" : {
    "primaryType" : {
      "provider" : "lucene",
      "columns" : "jcr:primaryType(STRING)",
      "kind" : "nodetype"
    },
    "mixinType" : {
      "provider" : "lucene",
      "columns" : "jcr:mixinTypes(STRING)",
      "kind" : "nodetype"
    },
    "path" : {
      "columns" : "jcr:path(PATH)",
      "provider" : "lucene"
    },
    "hasMember" : {
      "columns" : "pcdm:hasMember(STRING)",
      "provider" : "lucene"
    },
    "proxyFor" : {
      "columns" : "ore:proxyFor(STRING)",
      "provider" : "lucene"
    }
  },
  "reindexing" : {
    "async" : false
  },
  "node-types" : ["fedora-node-types.cnd"]
}
